
def perfect_num(num1,num2):
    sum_ = 0
    for i in range (num1,num2+1):
        for num in range (1,(i//2)+1):
            if  i % num == 0:
                sum_ += i 
        if sum_ == i:
            print(i," is a Perfect number!")
        else:
            print(i," is not a Perfect number!")        


number1 = int(input())
number2 = int(input())
perfect_num(number1,number2)
